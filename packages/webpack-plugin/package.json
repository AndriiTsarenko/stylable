{
  "name": "@stylable/webpack-plugin",
  "version": "2.1.1",
  "description": "Webpack (4.x) plugin for Stylable modules",
  "main": "cjs/index.js",
  "types": "cjs/index.d.ts",
  "scripts": {
    "clean": "rimraf ./cjs",
    "build": "ts-build ./src --cjs",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "mocha -r @ts-tools/node/r \"test/unit/*.spec.ts?(x)\" --watch-extensions ts,tsx",
    "test:e2e": "mocha -r @ts-tools/node/r \"test/e2e/*.spec.ts?(x)\" --watch-extensions ts,tsx --timeout 20000",
    "prepack": "yarn build"
  },
  "peerDependencies": {
    "@stylable/core": "^2.0.0",
    "webpack": "^4.6.0"
  },
  "dependencies": {
    "@stylable/module-utils": "^2.2.0",
    "@stylable/node": "^2.1.1",
    "@stylable/optimizer": "^2.1.0",
    "@stylable/runtime": "^2.1.0",
    "find-config": "^1.0.0",
    "webpack-sources": "^1.3.0"
  },
  "files": [
    "cjs",
    "src",
    "!src/tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/wix/stylable/tree/master/packages/webpack-plugin",
  "author": "Wix.com",
  "license": "BSD-3-Clause"
}
